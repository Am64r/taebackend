{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/generate-text",
      "methods": ["GET"],
      "dest": "/app.py"
    },
    {

      "src":"/hello-world",
      "methods":["GET"],
      "dest":"/app.py"
    },

    {
      "src": "/(.*)",
      "dest": "/app.py"
    }
  ]
}
